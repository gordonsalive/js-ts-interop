{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "./dist/",
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "es6",
        "allowJs": true
    },
    "exclude": [
      "node_modules"
    ]
}